import java.util.Scanner;
public class javaClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		
		int crab = 13;
		int fish = 14;
		
		//++crab <-- preincrementing
		//crab++ <-- postincrementing
		System.out.println("Current crab total = " + crab);
		System.out.println("Incremented crab total = " + ++crab); //pre-incrementing
		
		System.out.println("Current fish total = " + fish++); //post-incrementing
		System.out.println("Incremented fish total = " + fish);
	}

}
